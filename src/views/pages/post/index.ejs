<div id="posts-container">
  <% for(var i = 0; i < posts.length; i++) { %>
  <div class="card mb-4">
    <div class="card-body">
      <h5 class="card-title"><%= posts[i].title%></h5>
      <p class="card-text"><%- posts[i].body%></p>
      <a href="/post/<%= posts[i]._id%>" class="btn btn-secondary">More</a>
    </div>
  </div>
  <% } %>
</div>
